run = "python main.py"
entrypoint = "main.py"

[nix]
channel = "stable-23.11"

[env]
PYTHONPATH = "${PYTHONPATH}:/home/runner/workspace"
PORT = "8000"

[packager]
language = "python3"

[languages.python3]
pattern = "**/*.py"
syntax = "python"

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 8000
externalPort = 80
